{
    "title": "over-coding",
    "author": "_yuusha",
    "description": "代码之外的一些总结",
    "language": "zh-hans",
    "gitbook": ">=3.0.0",
    "styles": {
        "website": "styles/website.css",
        "ebook": "styles/ebook.css",
        "pdf": "styles/pdf.css",
        "mobi": "styles/mobi.css",
        "epub": "styles/epub.css"
    },
    "plugins": [
        "-lunr",
        "-search",
        "search-plus",
        "splitter",
        "-sharing",
        "sharing-plus",
        "tbfed-pagefooter",
        "anchor-navigation-ex",
        "alerts",
        "edit-link"
    ],
    "pluginsConfig": {
        "fontsettings": {
            "theme": "white",
            "family": "sans",
            "size": 2
        },
        "sharing": {
            "qq": true,
            "weibo": true,
            "qzone": true,
            "google": true,
            "all": ["facebook", "google", "twitter", "weibo", "qq", "qzone"]
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy; yumecoder.top 2019",
            "modify_label": "最后修改时间:",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "anchor-navigation-ex": {
            "multipleH1": false,
            "showLevel" : false
        },
         "edit-link": {
            "base": "https://github.com/ks4na/over-coding/edit/master",
            "label": "Edit This Page"
        }
    }
}